<%= form_for(@Monster) url: users_path, html: { multipart: true } do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label :exp %>
  <%= f.email_field :email, class: 'form-control' %>
  
<% end %>
  
