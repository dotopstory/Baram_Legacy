
 <div class="col-md-2">
    <%= link_to item_path(item) do %>
       <div class="squarebox">
          <div class="pid"> 
           <span class="label label-gray"><%= item.id %> </span>
          </div>
          <div class="pimageid"> 
           <span class="label label-green"><%= item.image_id %> </span>
          </div>
           
          <%= image_tag item.image_url %>
          <p> 
              <%= item.name %>
          </p>
       </div>
    <% end %>
 </div>
